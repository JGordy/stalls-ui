{
    "name": "@jgordy24/stalls-ui",
    "version": "0.2.10",
    "author": "JGordy",
    "license": "MIT",
    "main": "dist/index.js",
    "module": "dist/index.js",
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/JGordy/stalls-ui"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.17",
        "@fortawesome/free-brands-svg-icons": "^5.8.1",
        "@fortawesome/free-regular-svg-icons": "^5.8.1",
        "@fortawesome/free-solid-svg-icons": "^5.8.1",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "@jgordy24/react-hooks-lib": "^1.3.0",
        "node-sass-chokidar": "^1.3.4",
        "npm-run-all": "^4.1.5",
        "react": "^16.8.5",
        "react-dom": "^16.8.5",
        "react-scripts": "^3.0.1"
    },
    "scripts": {
        "build-css": "node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
        "start:react": "react-scripts start",
        "start": "npm-run-all -p watch-css start:react",
        "storybook": "start-storybook -p 9001 -c .storybook",
        "build:react": "react-scripts build",
        "build": "npm-run-all -s build:css build:react",
        "build:babel": "NODE_ENV=production babel src --out-dir dist --copy-files --presets=react,env,stage-2 --ignore spec.js,test.js,__snapshots__",
        "build:publish": "npm run build-css && rm -rf dist && npm run build:babel",
        "test": "CI=true react-scripts test --env=jsdom",
        "test:generate-output": "BABEL_ENV=test jest --json --outputFile=src/jest-test-results.json",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@storybook/addon-jest": "^5.0.3",
        "@storybook/react": "^5.0.1",
        "babel-cli": "^6.26.0",
        "babel-jest": "^24.8.0",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "enzyme": "^3.9.0",
        "enzyme-adapter-react-16": "^1.11.2",
        "eslint-plugin-react": "^7.12.4",
        "jest": "^24.8.0",
        "jest-cli": "^24.8.0",
        "prop-types": "^15.7.2"
    },
    "publishConfig": {
        "access": "public"
    }
}